{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://music.youtube.com/*",
                "https://music.youtube.com/*"
            ],
            "js": [
                "js/contentScript.js"
            ]
        }
    ]
}